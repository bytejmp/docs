{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "BookJMP",
  "colors": {
    "primary": "#16A34A",
    "light": "#07C983",
    "dark": "#15803D"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "docs",
        "icon": "square-terminal",
        "pages": [
          "introduction"
        ]
      }
    ]
  },
  "footer": {
    "socials": {
      "x": "https://x.com/ByteJMP",
      "github": "https://github.com/bytejmp"
    }
  },
  "api": {
    "playground": {
      "proxy": false
    }
  },
  "seo": {
    "indexing": "navigable"
  },
  "errors": {
    "404": {
      "redirect": false,
      "title": "Are You Lost?"
    }
  },
  "styling": {
    "codeblocks": "dark"
  },
  "appearance": {
    "strict": true,
    "default": "dark"
  }
}
